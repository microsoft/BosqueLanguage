    }

    recursive? method argMin<V=? where operator <(V, V)>(vf: recursive? fn(_: T) -> V): T
        requires release List<T>::s_size(this) != 0;
    {
        return List<T>::s_argmin[recursive?](this, vf);
    }

    recursive? method tryArgMin<V=? where operator <(V, V)>(vf: recursive? fn(_: T) -> V): T? {
        return List<T>::s_size(this) != 0 ? List<T>::s_argmin[recursive?](this, vf) : none;
