    }

    recursive? method tryArgMax<V=? where operator <(V, V)>(vf: recursive? fn(_: T) -> V): T? {
        return List<T>::s_size(this) != 0 ? List<T>::s_argmin[recursive?](this, vf) : none;
    }

    method {when T operator +(T, T)} sum(): T {
        return List<Int>::s_sum(this);
    }
    
    method {when T operator *(T, T)} product(): T {
