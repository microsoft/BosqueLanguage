    {
        return List<T>::s_min_ordtype(this);
    }

    method {when T operator <(T, T)} tryMinOf(): T? {
        return List<T>::s_size(this) != 0 ? List<T>::s_min_ordtype(this) : none;
    }

    method {when T KeyType} max(): T 
        requires release List<T>::s_size(this) != 0;
    {
        return List<T>::s_max(this);
    }

    method {when T KeyType} tryMax(): T? {
        return List<T>::s_size(this) != 0 ? List<T>::s_max(this) : none;
    }

     method {when T operator <(T, T)} maxOf(): T 
        requires release List<T>::s_size(this) != 0;
    {
        return List<T>::s_max(this);
    }

    method {when T operator <(T, T)} tryMaxOf(): T? {
