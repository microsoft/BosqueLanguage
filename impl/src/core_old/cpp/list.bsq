    private recursive? static s_indexoflast(l: List<T>, s: Int, e: Int, p: recursive? fn(_: T) -> Bool): Int # list_indexoflast

    private recursive? static s_indexofnot(l: List<T>, s: Int, e: Int, p: recursive? fn(_: T) -> Bool): Int # list_indexofnot
    private recursive? static s_indexoflastnot(l: List<T>, s: Int, e: Int, p: recursive? fn(_: T) -> Bool): Int # list_indexoflastnot

    private recursive? static {when T grounded KeyType} s_count_keytype(l: List<T>, v: T): Int # list_count_keytype
    private recursive? static {when T grounded KeyType} s_indexof_keytype(l: List<T>, s: Int, e: Int, v: T): Int # list_indexof_keytype
    private recursive? static {when T grounded KeyType} s_indexoflast_keytype(l: List<T>, s: Int, e: Int, v: T): Int # list_indexoflast_keytype

    private recursive? static {when T grounded KeyType} s_min(l: List<T>): T # list_min
    private recursive? static {when T operator <(T, T)} s_minof(l: List<T>): T # list_minof
    private recursive? static {when T grounded KeyType} s_max(l: List<T>): T # list_max
    private recursive? static {when T operator <(T, T)} s_maxof(l: List<T>): T # list_maxof

    private recursive? static s_argmin<V where operator <(V, V)>(l: List<T>, (vf: recursive? fn(_: T) -> V): T # list_argmin
    private recursive? static s_argmax<V where operator <(V, V)>(l: List<T>, (vf: recursive? fn(_: T) -> V): T # list_argmax

    private recursive? static {when T operator +(T, T)} s_sum(l: List<T>): T # list_sum
    private recursive? static {when T operator *(T, T)} s_product(l: List<T>): T # list_product

    private recursive? static s_filter(l: List<T>, p: recursive? fn(_: T) -> Bool): List<T> # list_filter
    private recursive? static s_filternot(l: List<T>, p: recursive? fn(_: T) -> Bool): List<T> # list_filternot

    private recursive? static s_oftype<U>(l: List<T>): List<U> # list_oftype
    private recursive? static s_cast<U>(l: List<T>): List<U> # list_cast
    private recursive? static s_slice(l: List<T>, s: Int, e: Int): List<T> # list_slice

    private recursive? static s_takewhile(l: List<T>, p: recursive? fn(_: T) -> Bool): List<T> # list_takewhile
    private recursive? static s_discardwhile(l: List<T>, p: recursive? fn(_: T) -> Bool): List<T> # list_discardwhile
    private recursive? static s_takeuntil(l: List<T>, p: recursive? fn(_: T) -> Bool): List<T> # list_takeuntil
    private recursive? static s_discarduntil(l: List<T>, p: recursive? fn(_: T) -> Bool): List<T> # list_discarduntil

    private static {when T KeyType} s_unique(l: List<T>): List<T> # list_unique
    private static s_reverse(l: List<T>): List<T> # list_reverse
    
    private recursive? static s_map<U>(l: List<T>, f: recursive? fn(_: T) -> U): List<U> # list_map
    private recursive? static s_mapindex<U>(l: List<T>, f: recursive? fn(_: T, _: Int) -> U): List<U> # list_mapindex

    private static {when T KeyType} s_projectWMap<U>(l: List<T>, m: Map<T, U>): List<U> # list_project
    private static {when T KeyType} s_tryProjectWMap<U>(l: List<T>, m: Map<T, U>): List<U?> # list_tryproject
    private static {when T KeyType} s_defaultProjectWMap<U>(l: List<T>, m: Map<T, U>, d: U): List<U> # list_defaultproject

    private static  s_zipindex(l: List<T>): List<#[Int, T]> # list_zipindex

