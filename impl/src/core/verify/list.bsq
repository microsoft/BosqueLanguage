    }

    recursive? method findTry(out? res: T, p: recursive? pred(_: T, _: Nat) -> Bool): Bool {
        let idx = List<T>::s_findIndexOf_PredicateHelper_Idx[recursive?](this, p);
        if(idx == end) {
