    recursive? method findTry(out? res: T, p: recursive? pred(_: T) -> Bool): Bool {
        let idx = List<T>::s_findIndexOf_PredicateHelper[recursive?](this, p);
        if(idx == end) {
            return false;
        }
        else {
            res = List<T>::s_unsafe_get(this, idx);
            return true;
