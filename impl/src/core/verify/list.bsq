    recursive? method findLast(p: recursive? pred(_: T) -> Bool): T {
        let idx = List<T>::s_findIndexOfLast_PredicateHelper[recursive?](this, p);
        check idx != end;
        return List<T>::s_unsafe_get(this, idx);
