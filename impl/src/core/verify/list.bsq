        else {
            let lastidx = ListOps::s_find_last_pred<T>[recursive?](this, p);

            if(idx !== lastidx) {
                return false;
            }
            else {
                res = ListOps::s_safe_get<T>(this, idx);
                return true;
            }
        }
    }

