    method popBack(): List<T> {
        check !ListFlatOps::s_empty<T>(this);

        return ListOps::s_slice<T>(this, 0, ListOps::s_size<T>(this) - 1);
    }

    method popFront(): List<T> {
        check !ListFlatOps::s_empty<T>(this);

        return ListOps::s_slice<T>(this, 1, ListOps::s_size<T>(this));
    }

    method set(i: Nat, v: T): List<T> {
        check i < ListOps::s_size<T>(this);

        let count = ListOps::s_size<T>(this);
        let vl = List<T>@{v};

        if(count == 1) {
            return vl;
        }
        else {
            if(i == 0) {
                return ListOps::s_concat2<T>(vl, ListOps::s_slice<T>(this, 1, count));
            }
            elif(i == count - 1) {
                return ListOps::s_concat2<T>(ListOps::s_slice<T>(this, 0, count - 1), vl);
            }
            else {
                let front = ListOps::s_slice<T>(this, 0, i);
                let back = ListOps::s_slice<T>(this, i + 1, count);

                return ListOps::s_concat2<T>(ListOps::s_concat2<T>(front, vl), back);
            }
        } 
    }

    method remove(i: Nat): List<T> {
        check i < ListOps::s_size<T>(this);

        let count = ListOps::s_size<T>(this);

        if(count == 1) {
            return List<T>@{};
        }
        else {
            if(i == 0) {
                return ListOps::s_slice<T>(this, 1, count);
            }
            elif(i == count - 1) {
                return ListOps::s_slice<T>(this, 0, count - 1);
            }
            else {
                let front = ListOps::s_slice<T>(this, 0, i);
                let back = ListOps::s_slice<T>(this, i + 1, count);

                return ListOps::s_concat2<T>(front, back);
            }
        } 
    }

