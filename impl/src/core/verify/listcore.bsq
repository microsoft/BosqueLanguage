            return ListOps::s_filter_idx<T>(l, pred(v: T, i: Nat): Bool => {
                return !ListOps::s_has_pred_idx<T>(l, pred(u: T, j: Nat): Bool => {
                    ListOps::s_and2(i != j, KeyType::equal<T>(u, v));
                });
            });
        }
    }

