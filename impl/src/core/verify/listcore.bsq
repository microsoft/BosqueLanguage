            return ListOps::s_and3(p[recursive?](ListFlatOps::s_3at0(l), bidx), p[recursive?](ListFlatOps::s_3at1(l), bidx + 1n), p[recursive?](ListFlatOps::s_3at2(l), bidx + 2n));
        }
        else {
            return ListOps::s_and4(p[recursive?](ListFlatOps::s_4at0(l), bidx), p[recursive?](ListFlatOps::s_4at1(l), bidx + 1n), p[recursive?](ListFlatOps::s_4at2(l), bidx + 2n), p[recursive?](ListFlatOps::s_4at3(l), bidx + 3n));
        }
    }

    recursive? function s_find_index_pred<T>(l: List<T>, bidx: Nat, p: recursive? pred(_: T) -> Bool): Nat? {
        if(ListFlatOps::s_empty<T>(l)) {
            return none;
        }
        elif(ListFlatOps::s_is1<T>(l)) {
            if(p[recursive?](ListFlatOps::s_1at0(l))) {
                return bidx;
            }
            else {
                return none;
            }
        }
        else(ListFlatOps::s_is2<T>(l)) {
            if(p[recursive?](ListFlatOps::s_2at0(l))) {
                return bidx;
            } 
            elif(p[recursive?](ListFlatOps::s_2at1(l))) {
                return bidx + 1n;
            }
            else {
                return none;
            }
        }
        elif(ListFlatOps::s_is3<T>(l)) {
            if(p[recursive?](ListFlatOps::s_3at0(l))) {
                return bidx;
            }
            elif(p[recursive?](ListFlatOps::s_3at1(l))) {
                return bidx + 1n;
            }
            elif(p[recursive?](ListFlatOps::s_3at2(l))) {
                return bidx + 2n;
            }
            else {
                return none;
            }
        }
        else {
            if(p[recursive?](ListFlatOps::s_4at0(l))) {
                return bidx;
            }
            elif(p[recursive?](ListFlatOps::s_4at1(l))) {
                return bidx + 1n;
            }
            elif(p[recursive?](ListFlatOps::s_4at2(l))) {
                return bidx + 2n;
            }
            elif(p[recursive?](ListFlatOps::s_4at3(l))) {
                return bidx + 3n;
            }
            else {
                return none;
            }
        }
    }

