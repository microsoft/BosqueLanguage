            return List<U>@{f[recursive?](ListFlatOps::s_4at0(l), bidx), f[recursive?](ListFlatOps::s_4at1(l), bidx + 1n), f[recursive?](ListFlatOps::s_4at2(l), bidx + 2n), f[recursive?](ListFlatOps::s_4at3(l), bidx + 3n)};
        }
    }

    recursive? function s_filter<T>(l: List<T>, p: recursive? pred(_: T) -> Bool): List<T> {
        if(ListFlatOps::s_empty<T>(l)) {
            return List<T>@{};
        }
        elif(ListFlatOps::s_is1<T>(l)) {
            if(p[recursive?](ListFlatOps::s_1at0(l))) {
                return l;
            }
            else {
                return List<T>@{};
            }
        }
        else(ListFlatOps::s_is2<T>(l)) {
            let at0 = ListFlatOps::s_2at0(l);
            let pat0 = p[recursive?](at0);
            let at1 = ListFlatOps::s_2at1(l);
            let pat1 = p[recursive?](at1);

            if(ListOps::s_and2(pat0, pat1) {
                return l;
            }
            elif(pat0) {
                return List<T>@{at0};
            }
            elif(pat1) {
                return List<T>@{at1};
            }
            else {
                return List<T>@{};
            }
        }
        elif(ListFlatOps::s_is3<T>(l)) {
            let at0 = ListFlatOps::s_3at0(l);
            let pat0 = p[recursive?](at0);
            let at1 = ListFlatOps::s_3at1(l);
            let pat1 = p[recursive?](at1);
            let at2 = ListFlatOps::s_3at2(l);
            let pat2 = p[recursive?](at2);

            if(ListOps::s_and3(pat0, pat1, pat2) {
                return l;
            }
            elif(ListOps::s_and2(pat0, pat1) {
                return List<T>@{at0, at1};
            }
            elif(ListOps::s_and2(pat0, pat2) {
                return List<T>@{at0, at2};
            }
            elif(ListOps::s_and2(pat1, pat2) {
                return List<T>@{at1, at2};
            }
            elif(pat0) {
                return List<T>@{at0};
            }
            elif(pat1) {
                return List<T>@{at1};
            }
            elif(pat2) {
                return List<T>@{at2};
            }
            else {
                return List<T>@{};
            }
        }
        else {
            let at0 = ListFlatOps::s_4at0(l);
            let pat0 = p[recursive?](at0);
            let at1 = ListFlatOps::s_4at1(l);
            let pat1 = p[recursive?](at1);
            let at2 = ListFlatOps::s_4at2(l);
            let pat2 = p[recursive?](at2);
            let at3 = ListFlatOps::s_4at3(l);
            let pat3 = p[recursive?](at3);

            if(ListOps::s_and4(pat0, pat1, pat2, pat3) {
                return l;
            }
            elif(ListOps::s_and3(pat1, pat2, pat3) {
                return List<T>@{at1, at2, at3};
            }
            elif(ListOps::s_and3(pat0, pat2, pat3) {
                return List<T>@{at0, at2, at3};
            }
            elif(ListOps::s_and3(pat0, pat1, pat3) {
                return List<T>@{at0, at1, at3};
            }
            elif(ListOps::s_and3(pat0, pat1, pat2) {
                return List<T>@{at0, at1, at2};
            }
