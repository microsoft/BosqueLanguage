//The core namespace implements the primitive concepts used in the bosque core language
namespace NSCore;

__internal entity ListFlatOps provides Object {
    __safe function s_empty<T>(l: List<T>): Bool # list_empty

    __safe function s_is1<T>(l: List<T>): Bool # flat_list_is1
    __assume_safe function s_1at0<T>(l: List<T>): T # flat_list_1at0

    __safe function s_is2<T>(l: List<T>): Bool # flat_list_is2
    __assume_safe function s_2at0<T>(l: List<T>): T # flat_list_2at0
    __assume_safe function s_2at1<T>(l: List<T>): T # flat_list_2at1

    __safe function s_is3<T>(l: List<T>): Bool # flat_list_is3
    __assume_safe function s_3at0<T>(l: List<T>): T # flat_list_3at0
    __assume_safe function s_3at1<T>(l: List<T>): T # flat_list_3at1
    __assume_safe function s_3at2<T>(l: List<T>): T # flat_list_3at2

    __safe function s_is4<T>(l: List<T>): Bool # flat_list_is4
    __assume_safe function s_4at0<T>(l: List<T>): T # flat_list_4at0
    __assume_safe function s_4at1<T>(l: List<T>): T # flat_list_4at1
    __assume_safe function s_4at2<T>(l: List<T>): T # flat_list_4at2
    __assume_safe function s_4at3<T>(l: List<T>): T # flat_list_4at3

    //equiv s_empty | s_is1 | s_is2 | s_is3 | s_is4
    __safe function s_isFlatList<T>(l: List<T>): Bool # list_isflat

    function s_at<T>(l: List<T>, i: Nat): T {
        if(ListFlatOps::s_is1<T>(l)) {
            return ListFlatOps::s_1at0<T>(l);
        }
        elif(ListFlatOps::s_is2<T>(l)) {
            if(i == 0) {
                return ListFlatOps::s_2at0<T>(l);
            }
            else {
                return ListFlatOps::s_2at1<T>(l);
            }
        }
        elif(ListFlatOps::s_is3<T>(l)) {
            if(i == 0) {
                return ListFlatOps::s_3at0<T>(l);
            }
            elif(i == 1) {
                return ListFlatOps::s_3at1<T>(l);
            }
            else {
                return ListFlatOps::s_3at2<T>(l);
            }
        }
        else {
            if(i == 0) {
                return ListFlatOps::s_4at0<T>(l);
            }
            elif(i == 1) {
                return ListFlatOps::s_4at1<T>(l);
            }
            elif(i == 2) {
                return ListFlatOps::s_4at2<T>(l);
            }
            else {
                return ListFlatOps::s_4at3<T>(l);
            }
        }
    }

    function s_slice_front<T>(l: List<T>, start: Nat): List<T> {
        if(start == 0) {
            return l;
        }
        elif(start == 1) {
            if(ListFlatOps::s_is4<T>(l)) {
                return List<T>@{ListFlatOps::s_4at1<T>(l), ListFlatOps::s_4at2<T>(l), ListFlatOps::s_4at3<T>(l)};
            }
            elif(ListFlatOps::s_is3<T>(l)) {
                return List<T>@{ListFlatOps::s_3at1<T>(l), ListFlatOps::s_3at2<T>(l)};
            }
            elif(ListFlatOps::s_is2<T>(l)) {
                return List<T>@{ListFlatOps::s_2at1<T>(l)};
            }
            else {
                return List<T>@{};    
            }
        }
        elif(start == 2) {
            if(ListFlatOps::s_is4<T>(l)) {
                return List<T>@{ListFlatOps::s_4at2<T>(l), ListFlatOps::s_4at3<T>(l)};
            }
            elif(ListFlatOps::s_is3<T>(l)) {
                return List<T>@{ListFlatOps::s_3at2<T>(l)};
            }
            else {
                return List<T>@{};    
            }
        }
        else(start == 3) {
            if(ListFlatOps::s_is4<T>(l)) {
                return List<T>@{ListFlatOps::s_4at3<T>(l)};
            }
            else {
                return List<T>@{};    
            }
        }
        else {
            return List<T>@{};
        }
    }

    function s_slice_back<T>(l: List<T>, end: Nat): List<T> {
        if(end == 0) {
            return List<T>@{};
        }
        elif(end == 3) {
            if(ListFlatOps::s_is4<T>(l)) {
                return List<T>@{ListFlatOps::s_4at0<T>(l), ListFlatOps::s_4at1<T>(l), ListFlatOps::s_4at2<T>(l)};
            }
            else {
                return l;    
            }
        }
        elif(end == 2) {
            if(ListFlatOps::s_is4<T>(l)) {
                return List<T>@{ListFlatOps::s_4at0<T>(l), ListFlatOps::s_4at1<T>(l)};
            }
            elif(ListFlatOps::s_is3<T>(l)) {
                return List<T>@{ListFlatOps::s_3at0<T>(l), ListFlatOps::s_3at1<T>(l)};
            }
            else {
                return l;    
            }
        }
        else(start == 1) {
            iif(ListFlatOps::s_is4<T>(l)) {
                return List<T>@{ListFlatOps::s_4at0<T>(l)};
            }
            elif(ListFlatOps::s_is3<T>(l)) {
                return List<T>@{ListFlatOps::s_3at0<T>(l)};
            }
            elif(ListFlatOps::s_is2<T>(l)) {
                return List<T>@{ListFlatOps::s_2at0<T>(l)};
            }
            else {
                return l;
            }
        }
        else {
            return l;
        }
    }

    function s_slice<T>(l: List<T>, start: Nat, end: Nat): List<T> {
        let ff = ListFlatOps::s_slice_front<T>(l, start);
        let ll = ListFlatOps::s_slice_back<T>(ff, end - start);

        return ll;
    }

    recursive? function s_has_pred_check<T>(l: List<T>, p: recursive? pred(_: T) -> Bool): Bool {
        if(ListOps::s_empty<T>(l)) {
            return false;
        }
        elif(ListFlatOps::s_is1<T>(l)) {
            return p[recursive?](ListFlatOps::s_1at0(l));
        }
        else(ListFlatOps::s_is2<T>(l)) {
            return ListOps::s_or2(p[recursive?](ListFlatOps::s_2at0(l)), p[recursive?](ListFlatOps::s_2at1(l)));
        }
        elif(ListFlatOps::s_is3<T>(l)) {
            return ListOps::s_or3(p[recursive?](ListFlatOps::s_3at0(l)), p[recursive?](ListFlatOps::s_3at1(l)), p[recursive?](ListFlatOps::s_3at2(l)));
        }
        else {
            return ListOps::s_or4(p[recursive?](ListFlatOps::s_4at0(l)), p[recursive?](ListFlatOps::s_4at1(l)), p[recursive?](ListFlatOps::s_4at2(l)), p[recursive?](ListFlatOps::s_4at3(l)));
        }
    }

    recursive? function s_has_pred_check_idx<T>(l: List<T>, bidx: Nat, p: recursive? pred(_: T, _: Nat) -> Bool): Bool {
        if(ListOps::s_empty<T>(l)) {
            return false;
        }
        elif(ListFlatOps::s_is1<T>(l)) {
            return p[recursive?](ListFlatOps::s_1at0(l), bidx);
        }
        else(ListFlatOps::s_is2<T>(l)) {
            return ListOps::s_or2(p[recursive?](ListFlatOps::s_2at0(l), bidx), p[recursive?](ListFlatOps::s_2at1(l), bidx + 1n));
        }
        elif(ListFlatOps::s_is3<T>(l)) {
            return ListOps::s_or3(p[recursive?](ListFlatOps::s_3at0(l), bidx), p[recursive?](ListFlatOps::s_3at1(l), bidx + 1n), p[recursive?](ListFlatOps::s_3at2(l), bidx + 2n));
        }
        else {
            return ListOps::s_or4(p[recursive?](ListFlatOps::s_4at0(l), bidx), p[recursive?](ListFlatOps::s_4at1(l), bidx + 1n), p[recursive?](ListFlatOps::s_4at2(l), bidx + 2n), p[recursive?](ListFlatOps::s_4at3(l), bidx + 3n));
        }
    }

    recursive? function s_all_pred_check<T>(l: List<T>, p: recursive? pred(_: T) -> Bool): Bool {
        if(ListFlatOps::s_empty<T>(l)) {
            return true;
        }
        elif(ListFlatOps::s_is1<T>(l)) {
            return p[recursive?](ListFlatOps::s_1at0(l));
        }
        else(ListFlatOps::s_is2<T>(l)) {
            return ListOps::s_and2(p[recursive?](ListFlatOps::s_2at0(l)), p[recursive?](ListFlatOps::s_2at1(l)));
        }
        elif(ListFlatOps::s_is3<T>(l)) {
            return ListOps::s_and3(p[recursive?](ListFlatOps::s_3at0(l)), p[recursive?](ListFlatOps::s_3at1(l)), p[recursive?](ListFlatOps::s_3at2(l)));
        }
        else {
            return ListOps::s_and4(p[recursive?](ListFlatOps::s_4at0(l)), p[recursive?](ListFlatOps::s_4at1(l)), p[recursive?](ListFlatOps::s_4at2(l)), p[recursive?](ListFlatOps::s_4at3(l)));
        }
    }

    recursive? function s_all_pred_check_idx<T>(l: List<T>, bidx: Nat, p: recursive? pred(_: T, _: Nat) -> Bool): Bool {
        if(ListFlatOps::s_empty<T>(l)) {
            return true;
        }
        elif(ListFlatOps::s_is1<T>(l)) {
            return p[recursive?](ListFlatOps::s_1at0(l), bidx);
