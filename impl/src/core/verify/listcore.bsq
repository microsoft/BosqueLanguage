    recursive function s_sum_rec<T unique Algebraic>(l: List<T>, acc: T, cpos: Nat, count: Nat): T {
        if(cpos == count) {
            return acc;
        }
        else {
            let nacc = acc + ListOps::s_safe_get<T>(l, cpos);
            return ListOps::s_sum_rec<T>[recursive](l, nacc, cpos + 1n, count);
        }
    }

