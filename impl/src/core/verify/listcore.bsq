    recursive function s_find_pred<T>(l: List<T>, bidx: Nat, p: recursive? pred(_: T) -> Bool): Nat? {
        let ff = ListConcatOps::s_concat_lropt_find_pred<T>[recursive](ListConcatOps::s_left<T>(l), bidx, p);
        if(ff !== none) {
