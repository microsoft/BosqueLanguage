        else {
            if(l === none) {
                return Vector1<T>{v};
            }
            elif(l.is<Vector1<T>>()) {
                return Vector2<T>{v, l.v0};
            }
            else {
                let ll = s_safeAs<ListRepr<T>, Vector2<T>>(l);
                return Vector3<T>{v, ll.v0, ll.v1};
            }
        }
    }

