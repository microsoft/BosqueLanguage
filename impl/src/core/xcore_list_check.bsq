    recursive? function s_transduce<T, E, U>(l: ListRepr<T>, env: E, op: recursive? fn(_: T, _: T) -> (|E, U|)): (|E, ListRepr<U>|) {
        if(l === none) {
            return (|env, none|);
        }
        elif(l.is<Vector1<T>>()) {
            let ll = s_safeAs<ListRepr<T>, Vector1<T>>(l);
            let e0, v0 = op[recursive?](env, ll.v0);
            return (|e0, Vector1<U>{v0}|);
