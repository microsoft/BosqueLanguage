        elif(l.is<Vector2<T>>()) {
            let ll = s_safeAs<ListRepr<T>, Vector2<T>>(l);
            let e0, v0 = op[recursive?](env, ll.v0);
            let e1, v1 = op[recursive?](e0, ll.v1);
