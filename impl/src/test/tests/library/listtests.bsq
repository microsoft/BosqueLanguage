entrypoint function castop(): Int {
    let l = List<Int|Bool|None>@{1, 2, 3, 2};
    let le = List<Int|Bool|None>@{};

    let lp = l.cast<Int>();
    check lp.size() == 4;
    check lp.front() == 1;
    check lp.back() == 2;
    check lp.sum() == 8;

    let lei = le.cast<Int>();
    check lei.empty();

    return 0; 
}

