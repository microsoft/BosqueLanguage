function fed5GCurrency(currency: Currency): Currency {
    if(\/(currency === USD, currency ===  EUR, currency === GBP, currency === CHF, currency === JPY, currency === AUD, currency === CAD)) {
        return currency;
    }
    else {
        return Currency::usd;
    }
}
