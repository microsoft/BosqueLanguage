namespace Main;

function createDefaultEmptyDB(): Database {
    return Database{
        "default",
        LogicalTime::zero,
        List<DBEntryFormat>{
            DBEntryFormat{true, "User", 0, none},
            DBEntryFormat{false, "Name", 1, none},
            DBEntryFormat{false, "ID", 2, /[a-z]/},
        },
        List<DBRecordData>{
        }
    };
}

chktest function createDB(): Bool {
    let dd = createDefaultEmptyDB();
    return dd.empty();
}

