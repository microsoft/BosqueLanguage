namespace Main;

chktest function sign_1(): Bool {
    return sign_impl(5I) == 1i;
}

chktest function sign_2(): Bool {
    return abs_impl(-3I) == 3I;
}

errtest function divok(x: BigInt, y: BigInt): Result<BigInt> {
    return div_fix_impl(x, y);
}
